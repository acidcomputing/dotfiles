            "folders": "/"
        },
        {
            "key": "║ {#35} {#0;37}memory  {#keys}║",
            "type": "memory"
        },
        {
            "key": "║ {#36}󰩟 {#0;37}network {#keys}║",
            "type": "localip",
            "format": "{ipv4} ({ifname})"
        },
        {
            "key": "╠═══════════╣",
            "type": "custom"
        },
        {
            "key": "║ {#39} {#0;37}colors  {#keys}║",
            "type": "colors",
            "symbol": "circle"
        },
        {
            "key": "╚═══════════╝",
